    "cookie-parser": "^1.4.6",
    "express": "4.x",
    "express-graphql": "^0.12.0",
    "express-handlebars": "^5.2.0",
    "express-session": "^1.17.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "patch-package": "^6.4.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.2",
    "type-graphql": "^1.1.1"
