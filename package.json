{
  "name": "typescript-postgres-apollo",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node lib/app.js",
    "build:server": "tsc -p tsconfig.json",
    "watch": "npm-run-all -p watch:* -c",
    "watch:client": "webpack --config webpack.config.cjs --mode development --watch",
    "watch:server": "nodemon -e ts --exec \"npm run build:server && npm run start\""
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
